<h1>Signup to a World of Superheros</h1>

<%= form_for @user, url: signup_path do |f|%>
  <div>
    <%= f.label :username%>
    <%= f.text_field :username,  autofocus: true%>
  </div>
  <div>
    <%= f.label :password%>
    <%= f.text_field :password%>
  </div>
  <div>
    <%= f.submit "Signup" %>
  </div>
<% end %>

<%= link_to "Already have an account? Login here.", login_path %>



